{
    "name": "hanishkumar/chatapp",   
    "description": "ChatApp is a PHP library designed to make it easy to consume and produce chat application",
    "keywords": ["Chat", "Chat Application", "Laravel Chat"],
	"homepage": "https://github.com/hanishkumar/chatapp",
    "type": "library",
	"license": "MIT",
    "authors": [
        {
            "name": "Hanish Kumar",
            "email": "info@hanishkumar.com",
            "homepage": "http://hanishkumar.com"            
        }
    ],   
    "require": {
        "php": ">=5.6.0"
    },
    "autoload": {        
        "psr-4": {           
            "Hanish\\ChatApp\\": "packages/hanishkumar/chatapp/src"
        }
    },
	 "autoload-dev": {        
        "psr-4": {           
            "Hanish\\ChatApp\\": "packages/hanishkumar/chatapp/src"
        }
    },
	 "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
	"minimum-stability": "dev",
    "prefer-stable": true,
}
